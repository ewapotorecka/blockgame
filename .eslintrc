{
	"parser": "@typescript-eslint/parser",
	"extends": [
		"ckeditor5"
	],
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"env": {
		"browser": true
	},
	"rules": {
        "semi": ["error", "always"],
		"quotes": ["error", "single"],
		"indent": ["error", "tab" ]
    },
	"plugins": [
		"@typescript-eslint"
	  ]
}
